{
  "name": "interactivecomic",
  "version": "2.0.1",
  "description": "interactive comic platform",
  "scripts": {
    "dev": " concurrently  \"npm run dts:w\" \"npm run tsc:w\" \"npm run watch\" ",
    "build": " concurrently \"npm run dts:w\" \"npm run tsc:w\" \"npm run publish\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "watch": "webpack -d --watch",
    "publish": "webpack -p",
    "dts": "tcm src",
    "dts:w": "tcm -w src"
  },
  "author": "Karl Kim",
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.4.5",
    "babel-loader": "6.2.2",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "font-awesome": "^4.6.1",
    "jquery": "2.2.2",
    "leaflet": "0.7.7",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-fontawesome": "^0.3.3",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "babel-preset-es2015-without-strict": "0.0.2",
    "classnames": "2.2.3",
    "concurrently": "2.0.0",
    "css-loader": "0.23.1",
    "exports-loader": "0.6.3",
    "file-loader": "0.8.5",
    "imports-loader": "0.6.5",
    "style-loader": "0.13.1",
    "typescript": "1.8.0",
    "typings": "0.7.9",
    "url-loader": "0.5.7",
    "webpack": "1.12.13"
  }
}
